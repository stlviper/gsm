                </div>
            </div>
        </div>
    </div>
    </div>
</div>
<!-- /Browser Warning Message -->
{{#contentFor "postJqueryScripts"}}
<script type="application/javascript">
$(document).ready(function() {

  var isFirefox = typeof InstallTrigger !== 'undefined';
  var isChrome = !!window.chrome;  
  var browserCookie = getCookie("browserDismiss");  
  if (isFirefox==false && isChrome==false && browserCookie != "true") {
  $('#checkBrowser').show();
  }
});
function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
    }
    return "";
} 
function dismissNotification(){
var now = new Date();
var time = now.getTime();
time += 43200 * 1000;
now.setTime(time);
document.cookie = 'browserDismiss=true; expires=' + now.toUTCString() + 
'; path=/';
$('#checkBrowser').hide();
}
</script>
